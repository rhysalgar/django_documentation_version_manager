{
  "name": "Django Documentation Version Manager",
  "version": "0.1",
  "description": "Automatically switch version of the Official Django Documentation website.",
  "permissions": [
    "declarativeContent",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://docs.djangoproject.com/"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "page_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "images/django_16.png",
      "32": "images/django_32.png",
      "48": "images/django_48.png",
      "128": "images/django_128.png"
    }
  },
  "icons": {
    "16": "images/django_16.png",
    "32": "images/django_32.png",
    "48": "images/django_48.png",
    "128": "images/django_128.png"
  },
  "manifest_version": 2
}